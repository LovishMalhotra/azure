			"properties": {
				"description": "Converting customer from raw schema to  staging layer by cleaning and sorting",
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "AzureSqlTableCustomer",
								"type": "DatasetReference"
							},
							"name": "srcCustomer",
